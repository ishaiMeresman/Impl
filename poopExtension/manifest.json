{
  "manifest_version": 2,
  "name": "Poop Converter",
  "description": "convert line separated values to comma separated",
  "version": "1.0",
  "browser_action": {
    "default_icon": "favicon.png",
    "default_popup": "poop.html"
  },
  "content_scripts": [ {
    "js": [ "jquery-3.2.1.min.js", "main.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "permissions": [
    "activeTab",
    "storage"
  ]
}