{
  "manifest_version": 2,
  "name": "weekNumber",
  "description": "An Extension that shows current week number!",
  "version": "1.0",
  "browser_action": {
//    "default_icon": "favicon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [ {
    "js": ["main.js"],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "permissions": [
    "activeTab",
    "storage"
  ]
}